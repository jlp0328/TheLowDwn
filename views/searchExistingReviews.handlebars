<div class="container col-md-6" style="margin-top: 65px">

<h1>Search</h1>

{{!-- Search for OkCupid User to Write Review --}}
<form>
  <div class="form-group">
    <label for="formGroupExampleInput" id="enterOkCupidUN">Enter OkCupid Username</label>
    <input type="text" class="form-control" id="readReviewSearch" placeholder="OkCupid Username" style="width: 350px">
     <a href="https://www.okcupid.com/home" id="lookupUser" target="_blank">I can't remember their username!</a>
  </div>

  <button type="button" class="btn btn-primary btn-lg outline findReview">Search for Reviews</button>

</form>

</fieldset>
</form>

</div>

<div class="container col-md-6" style="margin-top: 65px">

  <h2 style="margin-top: 35px" class="searchResults">Search Results</h2>

    {{#each dater}}

    <div class="container" style="margin-top: 100px">

        <div class="card col-md-6" id="daterDiv" style="padding: 30px; text-align: center">

            <img class="card-img-top" src="{{this.image}}">
              <div class="card-block">
              <p style="font-size: 15px" class="card-title">{{this.username}}</p>
              <p style="font-size: 15px" class="card-title">{{this.location}}</p>
              <p style="font-size: 15px" class="card-title">{{this.age}}</p>

              <a data-id="{{this._id}}" class="btn btn-primary">Read Reviews!</a>
              </div>

        </div>
        </div>


    {{/each}}

    </div>
